{
    "version": 2,
    "builds": [
        {
            "src": "main.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.9"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "main.py",
            "headers": {
                "access-control-allow-origin": "*"
            },
            "methods": [
                "GET",
                "POST",
                "PUT",
                "PATCH",
                "OPTIONS",
                "DELETE"
            ]
        },
        {
            "src": "/(.*)",
            "dest": "/controllers/bfs_controller.py",
            "headers": {
                "access-control-allow-origin": "*"
            },
            "methods": [
                "GET",
                "POST",
                "PUT",
                "PATCH",
                "OPTIONS",
                "DELETE"
            ]
        }
    ]
}